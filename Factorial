import java.util.Scanner;
public class Main
    {
        public static void main(String args[])
        {
            int n,fact=1,i=1;
            Scanner s=new Scanner(System.in);
            System.out.println("enter n value");
            n=s.nextInt();
            while(i<=n)
            {
                fact=fact*i;
                i++;
            }
            System.out.println("factorial of a number="+fact);
            
            
        }
    }

