class This
{
    int m,n;
    void sum (int m,int n)
    {
        this.m=m;
        this.n=n;
        System.out.println("the addition of two numbers="+(m+n));
    }
}
 public class Main
 {
     public static void main(String args[])
     {
         This t=new This();
         t.sum(10,13);
     }
 }
